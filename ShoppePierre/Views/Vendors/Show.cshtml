@using ShoppePierre.Models;
@{
  Layout = "_Layout";
}

<h2> Shoppe Pierre Vendor: Each List Order(s)</h2>
<ol>
@foreach(Order order in Model["orders"])
{
    <li>
    <a href ='/vendors/@Model["vendor"].Id/orders/@order.Id'>@order.Name</a>
    <a href ='/vendors/@Model["vendor"].Id/orders/@order.Id'> @order.Email</a>
    <a href ='/vendors/@Model["vendor"].Id/orders/@order.Id'> @order.PhoneNumber</a></li>
    
}
</ol>
<a href='/'>Go Back Home</a>
<a href='/vendors'>Go Back Vendors</a>
<a href='/vendors/@Model["vendor"].Id/orders/new'>Orders Home</a>
